    "vue use .md",
    "vue import .md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hileez/vite-plugin-vue-mdsfc.git"
  },
  "homepage": "https://github.com/hileez/vite-plugin-vue-mdsfc",
