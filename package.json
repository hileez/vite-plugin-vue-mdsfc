  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "prettier": "^3.7.1",
